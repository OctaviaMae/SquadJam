<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PointerLayout"><attr format="dimension" name="pointerSize"/><attr format="dimension" name="pointerMargin"/><attr format="color" name="pointerFill"/></declare-styleable>
    <declare-styleable name="RadScrollView"><attr format="boolean" name="fillViewport"/><attr name="scrollMode"><flag name="none" value="0"/><flag name="vertical" value="1"/><flag name="horizontal" value="2"/><flag name="both" value="3"/></attr></declare-styleable>
    <declare-styleable name="RadScrollViewTheme"><attr format="reference" name="radScrollViewStyle"/></declare-styleable>
    <dimen name="legend_stroke_width">1dp</dimen>
    <style name="RadScrollViewStyle">
        <item name="fillViewport">false</item>
        <item name="scrollMode">vertical</item>
    </style>
    <style name="RadScrollViewTheme">
        <item name="radScrollViewStyle">@style/RadScrollViewStyle</item>
    </style>
</resources>